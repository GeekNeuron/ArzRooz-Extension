{
  "manifest_version": 3,
  "name": "ArzRooz",
  "version": "1.1",
  "description": "نمایش لحظه‌ای نرخ طلا، ارز و ارزهای دیجیتال با ArzRooz",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://kifpool.me/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
